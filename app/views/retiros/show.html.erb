<h1>Ver datos retiro</h1>
<br/>
<div class="row">
  <div class="col-lg-4">
    <div class="form-group">
      <label>Cliente</label>
      <%= @retiro.cliente.name_surename %>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-lg-4">
    <div class="form-group">
      <label>Fecha</label>
      <%= @retiro.fecha %>
    </div>
  </div>
  <div class="row">
    <div class="col-lg-4">
      <div class="form-group">
        <label>Hora</label>
        <%= @retiro.hora %>
      </div>
    </div>
  </div>
  <div class="col-lg-4">
    <div class="form-group">
      <label>Estado</label>
      <%= @retiro.estado %>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-lg-12 text-right">
    <%= link_to 'Retiros', retiros_path, class:"btn btn-default" %>
    <%= link_to 'Editar', edit_retiro_path(@retiro), class: "btn btn-success" %>
  </div>
</div>

